<!-- LIDAR Sensor -->
<link name="lidar_link">
  <inertial>
    <mass value="0.05"/> <!-- Reduced mass -->
    <inertia ixx="5e-7" ixy="0" ixz="0" iyy="5e-7" iyz="0" izz="5e-7"/> 
  </inertial>
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/> 
    <geometry>
      <cylinder length="0.05" radius="0.025"/> <!-- Scaled dimensions -->
    </geometry>
    <material name="light_pink">
      <color rgba="1.0 0.7529 0.7961 1.0"/> 
    </material>
    <gazebo reference="lidar_link">
      <material>
        <ambient>1 0.7529 0.7961 1</ambient> 
      </material>
      <sensor name="laser" type="ray">
      <pose> 0 0 0 0 0 0 </pose>
      <visualize>true</visualize>
      <update_rate>10</update_rate>
      <ray> 
        <scan>
          <horizontal>
            <samples>360</samples>
            <min_angle>-3.14</min_angle>
            <max_angle>3.14</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.15</min>
          <max>6</max>
          </range>
        </ray>
      </sensor>
    </gazebo>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <cylinder length="0.05" radius="0.025"/>
    </geometry>
  </collision>
</link>
<joint name="lidar_joint" type="fixed">
